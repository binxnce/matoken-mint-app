    "@walletconnect/qrcode-modal": "^1.2.1",
    "apexcharts": "3.18.0",
    "axios": "^0.21.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-plugin-styled-components": "^1.10.7",
    "cookie-cutter": "^0.2.0",
    "downshift": "^2.2.3",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "ethers": "^5.0.17",
    "ethers-typed-data-legacy": "^0.1.0",
    "etherspot": "1.2.1",
