{
  "name": "dank-web",
  "version": "1.0.0",
  "description": "Best meme website",
  "main": "pages/_app.js",
  "scripts": {
    "dev": "next -p 7777",
    "build": "next build",
    "debug": "next dev -p 7777",
    "format": "prettier --write \"./**/*.js\"",
    "start": "next start -p \"$FRONTEND_PORT\"",
    "test": "NODE_ENV=test jest --watch",
    "dev:setup": "node scripts/devSetup.js",
    "lint": "eslint ."
  },
  "author": "Pillar Project Team",
  "license": "ISC",
